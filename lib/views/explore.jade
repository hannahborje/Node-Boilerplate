extends layout

block head
    style(type='text/css')
        body {
            padding-top: 60px;
            padding-bottom: 40px;
        }

block content
    include nav-fake-logged-in
    .container-fluid
        .row-fluid
            .span3
              accordion#accordion-left
                .accordion-group
                    .accordion-heading
                        a.accordion-toggle(href="#collapseTwo", data-toggle="collapse", data-parent="#accordion-left")
                            | Dina meddelanden >>
                        .accordion-body.collapse.in#collapseTwo
                            .accordion-inner
                                ul.nav#inbox-list
                                   h6
                                     | Från: hannah@student.se
                                   p
                                      | Hej Per!
                                      br
                                      br
                                      | Vad roligt att du också har gått med i Programmeringsklubben!
                                      br
                                      br
                                      | Hälsningar, Hannah


            include popupmsg
            .span7
                .hero-unit
                    .row-fluid
                        .columnA.pull-left
                            table.table
                                //caption
                                  | Rubrik
                                thead
                                    h2
                                        a#name(href='#', data-type='text', data-pk='1', data-url='/post', data-title='Förnamn')
                                            | #{"Förnamn"}
                                        a#surname(href='#', data-type='text', data-pk='1', data-url='/post', data-title='Efternamn')
                                            | #{"Efternamn"}
                                tr
                                    td
                                        p
                                            | ORT
                                            br
                                            | ÅLDER
                                            br
                                            | YRKE
                                            br
                                            | FÖRETAG
                                            br
                                            | UTBILDNING
